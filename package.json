{
  "name": "ng-seed",
  "version": "0.8.0-beta.2",
  "private": true,
  "description": "> angular项目基础框架，解决了前后端分离后，前端代码打包合并、资源文件CDN分离部署、单点登录、密码加密、服务接口跨域、SPA页面SEO等问题",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --hot --inline --quiet --open --config ./build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.config.js",
    "build:dll": "webpack --config build/webpack.dll.conf.js",
    "fecs:check": "fecs check src --reporter=baidu",
    "fecs:fix": "fecs format src --replace --safe=high",
    "seed": "npm run seed:changelog && npm run seed:jsdoc && npm run seed:authors && npm run seed:site",
    "seed:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "seed:jsdoc": "jsdoc -c ./scripts/jsdoc.conf.json",
    "seed:site": "node ./scripts/gh-pages.js",
    "seed:authors": "node ./scripts/authors.js",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.15",
    "markdown-it": "^8.3.1",
    "mkdirp": "^0.5.1",
    "multiparty": "^4.1.3",
    "ng-annotate": "^1.2.1",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^4.5.3",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^1.0.0",
    "postcss-sprites": "^4.0.0",
    "remove-webpack-plugin": "^1.2.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1",
    "webpack-uglify-parallel": "^0.1.3"
  },
  "dependencies": {
    "angular": "1.2.29",
    "angular-animate": "1.2.29",
    "angular-bindonce": "^0.3.1",
    "angular-lazy-image": "0.3.4",
    "angular-resource": "1.2.29",
    "angular-sanitize": "1.2.29",
    "angular-tree-control": "0.2.28",
    "angular-ui-router": "0.2.18",
    "echarts": "^3.3.2",
    "echarts-wordcloud": "^1.0.3",
    "file-uploader": "^1.0.0",
    "marked": "^0.3.6",
    "raw-loader": "^0.5.1",
    "sharp": "^0.18.1"
  },
  "optionalDependencies": {
    "conventional-changelog": "^1.1.3",
    "conventional-changelog-cli": "^1.3.1",
    "fecs": "^1.2.2",
    "gh-pages": "^1.0.0",
    "husky": "^0.13.3",
    "jsdoc": "^3.4.3",
    "validate-commit-msg": "^2.12.1"
  },
  "repository": "git+https://github.com/jm-team/ng-seed.git",
  "keywords": [
    "angularjs",
    "webpack",
    "angularjs-seed",
    "ng-seed"
  ],
  "bugs": {
    "url": "https://github.com/jm-team/ng-seed/issues"
  },
  "homepage": "https://github.com/jm-team/ng-seed#readme"
}
